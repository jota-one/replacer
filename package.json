{
  "name": "@jota-one/replacer",
  "version": "0.5.0",
  "type": "module",
  "description": "A configurable string replacer tool",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/*",
    "index.d.ts"
  ],
  "scripts": {
    "build": "node build.cjs",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jota-one/replacer.git"
  },
  "keywords": [
    "string",
    "replace",
    "replacer",
    "placeholder"
  ],
  "author": "Juniors at Work",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jota-one/replacer/issues"
  },
  "homepage": "https://github.com/jota-one/replacer#readme",
  "devDependencies": {
    "auto": "^10.43.0",
    "ava": "^3.12.1",
    "esbuild": "^0.6.27",
    "esm": "^3.2.25"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
